{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run start",
    "start": "rm -r docs/tutorial/1.mdx docs/tutorial/2.mdx docs/api 2>/dev/null; docusaurus start --port $(detect 3000)",
    "build": "rm -r docs/tutorial/1.mdx docs/tutorial/2.mdx docs/api 2>/dev/null; docusaurus build && cp _redirects build/",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "clean": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "spellcheck": "cspell \"docs/**/*.md*\"",
    "typecheck": "tsc",
    "test": "cspell \"docs/**/*.md*\" && node test.js"
  },
  "dependencies": {
    "@docusaurus/core": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@mdx-js/react": "2.3.0",
    "clsx": "1.2.1",
    "prism-react-renderer": "1.3.5",
    "raw-loader": "4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "convex-ai-chat": "workspace:*",
    "convex-analytics": "workspace:*",
    "convex": "workspace:*",
    "docusaurus-theme-mdx-v2": "0.1.2",
    "@docusaurus/theme-common": "2.3.1",
    "heroicons": "^2.0.16",
    "react-dropdown": "~1.11.0",
    "react-instantsearch-hooks-web": "~6.47.3",
    "algoliasearch": "~4.20.0",
    "react-markdown": "~8.0.7",
    "@types/react": "^18.0.0",
    "prettier": "3.2.5"
  },
  "devDependencies": {
    "docusaurus-plugin-typedoc": "npm:@convex-dev/docusaurus-plugin-typedoc@0.19.2",
    "typedoc-plugin-markdown": "npm:@convex-dev/typedoc-plugin-markdown@3.15.6",
    "@docusaurus/module-type-aliases": "2.3.1",
    "@tsconfig/docusaurus": "1.0.6",
    "typescript": "~5.0.3",
    "webpack": "5.94.0",
    "@docusaurus/types": "2.3.1",
    "cspell": "^6.30.2",
    "detect-port": "^1.5.1",
    "@types/node": "^18.17.0",
    "@docusaurus/theme-classic": "~2.4.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
